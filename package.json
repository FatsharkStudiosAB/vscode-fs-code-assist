{
  "name": "fatshark-code-assist",
  "displayName": "Fatshark Code Assist",
  "description": "",
  "version": "0.0.5",
  "publisher": "Fatshark",
  "engines": {
    "vscode": "^1.62.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuelBlanc/vscode-vtide.git"
  },
  "author": {
    "url": "http://www.fatshark.se",
    "name": "Fatshark",
    "email": "tools@fatshark.se"
  },
  "categories": [
    "Debuggers",
    "Programming Languages"
  ],
  "keywords": [
    "fatshark",
    "stingray",
    "debugger",
    "lua"
  ],
  "activationEvents": [
    "onCommand:fatshark-code-assist.stingrayConnect",
    "onCommand:fatshark-code-assist.stingrayRecompile",
    "onLanguage:lua",
    "onLanguage:sjson"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fatshark-code-assist.stingrayConnect",
        "title": "Connect All",
        "category": "Stingray",
        "icon": "$(run-all)"
      },
      {
        "command": "fatshark-code-assist.stingrayDisconnect",
        "title": "Disconnect",
        "category": "Stingray",
        "icon": "$(close)"
      },
      {
        "command": "fatshark-code-assist.stingrayDisconnectAll",
        "title": "Disconnect All",
        "category": "Stingray",
        "icon": "$(close-all)"
      },
      {
        "command": "fatshark-code-assist.stingrayRecompile",
        "title": "Recompile and Reload Sources",
        "category": "Stingray",
        "icon": "$(server-environment)"
      },
      {
        "command": "fatshark-code-assist.stingrayReloadSources",
        "title": "Reload Sources",
        "category": "Stingray",
        "icon": "$(repo-sync)"
      },
      {
        "command": "fatshark-code-assist.stingrayLua",
        "title": "Execute Lua",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.executeSelection",
        "title": "Execute Selection",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.executeFile",
        "title": "Execute File",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.stingrayCommand",
        "title": "Command",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.stingrayCommandTarget",
        "title": "Execute Command on Target",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.stingrayLuaTarget",
        "title": "Execute Lua on Target",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.selectionTarget",
        "title": "Execute Selection on Target",
        "category": "Stingray"
      },
      {
        "command": "fatshark-code-assist.attachDebugger",
        "title": "Attach Debugger",
        "category": "Stingray",
        "icon": "$(debug-alt)"
      },
      {
        "command": "fatshark-code-assist.detachDebugger",
        "title": "Detach Debugger",
        "category": "Stingray",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "fatshark-code-assist.refreshTargets",
        "title": "Refresh list",
        "category": "Stingray",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "fatshark-code-assist.stingrayCommandTarget",
          "when": "false"
        },
        {
          "command": "fatshark-code-assist.executeSelection",
          "when": "false"
        },
        {
          "command": "fatshark-code-assist.stingrayLuaTarget",
          "when": "false"
        },
        {
          "command": "fatshark-code-assist.selectionTarget",
          "when": "false"
        },
        {
          "command": "fatshark-code-assist.attachDebugger",
          "when": "false"
        },
        {
          "command": "fatshark-code-assist.refreshTargets",
          "when": "false"
        },
        {
          "command": "fatshark-code-assist.stingrayDisconnect",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "command": "fatshark-code-assist.executeSelection",
          "group": "Stingray",
          "when": "editorHasSelection"
        }
      ],
      "view/title": [
        {
          "command": "fatshark-code-assist.stingrayRecompile",
          "group": "navigation",
          "when": "view == fs-code-assist-targets"
        },
        {
          "command": "fatshark-code-assist.stingrayReloadSources",
          "group": "navigation",
          "when": "view == fs-code-assist-targets"
        },
        {
          "command": "fatshark-code-assist.refreshTargets",
          "group": "navigation",
          "when": "view == fs-code-assist-clients"
        }
      ],
      "view/item/context": [
        {
          "command": "fatshark-code-assist.attachDebugger",
          "when": "viewItem == connected-client",
          "group": "inline@1"
        },
        {
          "command": "fatshark-code-assist.stingrayDisconnect",
          "when": "viewItem == connected-client",
          "group": "inline@2"
        },
        {
          "command": "fatshark-code-assist.stingrayCommandTarget",
          "when": "viewItem == connected-client"
        },
        {
          "command": "fatshark-code-assist.stingrayLuaTarget",
          "when": "viewItem == connected-client"
        },
        {
          "command": "fatshark-code-assist.selectionTarget",
          "when": "viewItem == connected-client"
        },
        {
          "command": "fatshark-code-assist.stingrayConnect",
          "when": "viewItem == connection-target",
          "group": "inline@1"
        },
        {
          "command": "fatshark-code-assist.stingrayDisconnect",
          "when": "viewItem == connection-target"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Fatshark Code Assist",
      "properties": {
        "StingrayLua.toolchainPath": {
          "type": "string",
          "scope": "application",
          "default": "C:/BitSquidBinaries",
          "description": "Toolchain name, e.g. 'C:/BitSquidBinaries'"
        },
        "StingrayLua.toolchainName": {
          "type": "string",
          "scope": "application",
          "default": "vermintide2",
          "description": "Toolchain name, e.g. 'vermintide2'"
        },
        "StingrayLua.platform": {
          "type": "string",
          "scope": "application",
          "default": "win32",
          "description": "Platform name, e.g. 'win32', `xb1`, `ps4`"
        }
      }
    },
    "languages": [
      {
        "id": "stingray-output",
        "mimetypes": [
          "text/x-code-output"
        ]
      },
      {
        "id": "crashlog",
        "aliases": [
          "Stingray Crashlog"
        ],
        "extensions": [
          ".log"
        ],
        "configuration": "./language-configuration-crashlog.json"
      },
      {
        "id": "lua",
        "aliases": [
          "Stingray Lua"
        ],
        "extensions": [
          ".lua"
        ]
      },
      {
        "id": "sjson",
        "aliases": [
          "Simplified JSON"
        ],
        "filenames": [
          "settings.ini"
        ],
        "extensions": [
          ".bsi",
          ".component",
          ".config",
          ".dcc_asset",
          ".decals",
          ".entity",
          ".flow_editor",
          ".flow",
          ".font",
          ".import_options",
          ".landscape",
          ".level",
          ".material",
          ".mouse_cursor",
          ".network_config",
          ".package",
          ".particle_editor",
          ".particles",
          ".physics_properties",
          ".physics",
          ".plugin",
          ".render_config",
          ".script_flow_nodes",
          ".shader_node",
          ".shader_source",
          ".shader",
          ".shading_environment_template",
          ".shading_environment",
          ".sjson",
          ".sound_environment",
          ".stingray_layout",
          ".stingray_plugin",
          ".stingray_project",
          ".strings",
          ".surface_properties",
          ".texture_category",
          ".texture",
          ".timpani_bank",
          ".timpani_master",
          ".type",
          ".unit_anim",
          ".unit",
          ".volume_type"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "stingray-output",
        "scopeName": "source.stingray-output",
        "path": "./syntaxes/stingray-output.json"
      },
      {
        "language": "crashlog",
        "scopeName": "source.crashlog",
        "path": "./syntaxes/crashlog.json"
      },
      {
        "language": "lua",
        "scopeName": "source.fatshark.lua",
        "path": "./syntaxes/lua-stingray.json"
      },
      {
        "language": "sjson",
        "scopeName": "source.sjson",
        "path": "./syntaxes/sjson.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fs-code-assist",
          "title": "FS Code Assist",
          "icon": "resources/fs-code-assist.svg"
        }
      ]
    },
    "views": {
      "fs-code-assist": [
        {
          "id": "fs-code-assist-targets",
          "name": "Connection Targets"
        },
        {
          "id": "fs-code-assist-clients",
          "name": "Connected Clients"
        }
      ]
    },
    "breakpoints": [
      {
        "language": "lua"
      }
    ],
    "debuggers": [
      {
        "type": "stingray_lua",
        "label": "Stingray Lua",
        "program": "./out/stingray-debug-adapter.js",
        "runtime": "node",
        "configurationSnippets": [
          {
            "label": "Stingray: Application (Lua)",
            "description": "A new configuration for launching and debugging a Stingray project.",
            "body": {
              "type": "stingray_lua",
              "request": "attach",
              "name": "${1:My application name}",
              "toolchain": "C:/BitSquidBinaries/${2:vermintide2}",
              "ip": "127.0.0.1",
              "port": 14000
            }
          }
        ],
        "configurationAttributes": {
          "attach": {
            "required": [
              "ip",
              "port",
              "toolchain"
            ],
            "properties": {
              "ip": {
                "type": "string",
                "description": "IP address of the device running the Stingray engine.",
                "default": "127.0.0.1"
              },
              "port": {
                "type": "number",
                "description": "Port used by the engine for console connections.",
                "default": 14000
              },
              "toolchain": {
                "type": "string",
                "description": "the full directory where your toolchain resides (e.g. c:/project/mytoolchain)",
                "toolchain": "C:/BitSquidBinaries/vermintide2"
              }
            }
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "package": "vsce package",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "simplified-json": "^0.3.0",
    "vscode-debugadapter": "^1.51.0",
    "vscode-debugprotocol": "^1.51.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vsce": "^2.5.1"
  }
}
